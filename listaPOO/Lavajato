package exLavaJato;


public class Lavajato {
    private String nome;
    private double valor;
    private double tamanho;
    
        public Lavajato(String nome, double valor, double tamanho) {
            this.nome = nome;
            this.valor = valor;
            this.tamanho = tamanho;
        }
        
        public double getValor() {
            return valor;
        
        }
        
        public double getTamanho() {
            return tamanho;
        }
        
        public String getNome() {
            return nome;
        }

        public double sujeiraPesada(){
            return valor = valor * 1.1; // reajuste de 10% padr√£o
        }

}
    
